# =============================================================================
# Orbitport Oracle Environment Variables
# =============================================================================
# Copy this file to .env and fill in the values

# =============================================================================
# Network Configuration
# =============================================================================
RPC_URL=
PRIVATE_KEY=

# =============================================================================
# FeedManager Deployment (Required)
# =============================================================================
FEED_VERIFIER_ADDRESS=0x
PAUSER_REGISTRY_ADDRESS=0x
FEED_DEPLOYER_ADDRESS=0x

# Optional: Defaults to deployer address if not set
OWNER_ADDRESS=0x

# =============================================================================
# VRFAdapter Deployment (Required after FeedManager is deployed)
# =============================================================================
FEED_MANAGER_PROXY_ADDRESS=0x

# Optional: Defaults to 1
BEACON_ID=1

# =============================================================================
# Full Deployment Options (Optional)
# =============================================================================
# Comma-separated feed IDs to support initially
INITIAL_SUPPORTED_FEEDS=1,2,3

# Comma-separated publisher addresses to whitelist initially
INITIAL_PUBLISHERS=0x,0x

# =============================================================================
# Admin Operations
# =============================================================================
VRF_ADAPTER_ADDRESS=0x
